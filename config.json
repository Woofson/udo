{
    "start_page": "https://www.google.com",
    "default_search_engine": "g",
    "website_color_scheme": "dark",
    "statusbar_visible_by_default": false,
    "clock_format": "yyyy-MM-dd HH:mm",
    "downloads_path": "~/Downloads",
    "prioritize_https": true,
    "chromium_flags": [
        "--force-dark-mode"
    ],
    "!bangs": {
        "g": "https://www.google.com/search?q=%s",
        "ddg": "https://duckduckgo.com/?q=%s",
        "yt": "https://www.youtube.com/results?search_query=%s"
    },
    "!!bangs": {
        "gh": "https://github.com",
        "qt": "https://doc.qt.io/"
    },
    "!!!bangs": {
        "q": "quit",
        "tn": "tab_new",
        "tc": "tab_close",
        "tp": "tab_prev",
        "tx": "tab_next",
        "about": "about",
        "max": "maximize",
        "min": "minimize",
        "norm": "normalize",
        "clh": "clear_history",
        "clc": "clear_cookies",
        "clx": "clear_cache",
        "help": "help"
    },
    "shortcuts": {
        "toggle_do_bar": "Ctrl+D",
        "toggle_status_bar": "Ctrl+S",
        "quit": "Ctrl+Q",
        "new_tab": "Ctrl+T",
        "about": "Ctrl+Shift+A"
    }
}
